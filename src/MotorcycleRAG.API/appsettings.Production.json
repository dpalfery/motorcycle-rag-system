{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.ApplicationInsights": "Warning",
      "Azure": "Warning",
      "MotorcycleRAG": "Information"
    }
  },
  "AzureAI": {
    "Models": {
      "MaxTokens": 4096,
      "Temperature": 0.1,
      "TopP": 1.0
    },
    "Retry": {
      "MaxRetries": 3,
      "BaseDelaySeconds": 2,
      "MaxDelaySeconds": 60,
      "UseExponentialBackoff": true
    }
  },
  "Search": {
    "BatchSize": 100,
    "MaxSearchResults": 50,
    "EnableHybridSearch": true,
    "EnableSemanticRanking": true
  },
  "ApplicationInsights": {
    "EnableTelemetry": true,
    "EnablePerformanceCounters": true,
    "ApplicationName": "MotorcycleRAG"
  }
}